<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyReportLedgerTable</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:ASCII</SVEXPORTFORMAT>
                <SVFROMDATE>{fromDate}</SVFROMDATE>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyReportLedgerTable">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>MyPart</PARTS>
                    </FORM>
                    <PART NAME="MyPart">
                        <LINES>MyLine</LINES>
                        <REPEAT>MyLine : MyCollection</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLine">
                        <FIELDS>FldGuid,FldName,FldParent,FldIsRevenue,FldIsDeemedPositive,FldOpeningBalance,FldDescription,FldMailingName,FldMailingAddress,FldMailingState,FldMailingCountry,FldMailingPincode,FldPAN,FldGSTN,FldGSTRegistrationType,FldGSTTypeOfSupply,FldGSTDutyHead,FldTaxRate,FldEOL</FIELDS>
                    </LINE>
                    <FIELD NAME="FldGuid">
                        <SET>$Guid</SET>
                    </FIELD>
                    <FIELD NAME="FldName">
                        <SET>$$StringFindAndReplace:$Name:'"':'""'</SET>
                    </FIELD>
                    <FIELD NAME="FldParent">
                        <SET>$$StringFindAndReplace:$Parent:'"':'""'</SET>
                    </FIELD>
                    <FIELD NAME="FldIsRevenue">
                        <SET>if $IsRevenue then 1 else 0</SET>
                    </FIELD>
                    <FIELD NAME="FldIsDeemedPositive">
                        <SET>if $IsDeemedPositive then 1 else 0</SET>
                    </FIELD>
                    <FIELD NAME="FldOpeningBalance">
                        <SET>$$StringFindAndReplace:(if $$IsDebit:$OpeningBalance then -$$NumValue:$OpeningBalance else $$NumValue:$OpeningBalance):"(-)":"-"</SET>
                    </FIELD>
                    <FIELD NAME="FldDescription">
                        <SET>if $$IsEmpty:$Description then $$StrByCharCode:245 else $$StringFindAndReplace:$Description:'"':'""'</SET>
                    </FIELD>
                    <FIELD NAME="FldMailingName">
                        <SET>if $$IsEmpty:$MailingName then $$StrByCharCode:245 else $$StringFindAndReplace:$MailingName:'"':'""'</SET>
                    </FIELD>
                    <FIELD NAME="FldMailingAddress">
                        <SET>if $$IsEmpty:$Address then $$StrByCharCode:245 else $$StringFindAndReplace:($$FullList:Address:$Address):'"':'""'</SET>
                    </FIELD>
                    <FIELD NAME="FldMailingState">
                        <SET>if $$IsEmpty:$LedStateName then $$StrByCharCode:245 else $LedStateName</SET>
                    </FIELD>
                    <FIELD NAME="FldMailingCountry">
                        <SET>if $$IsEmpty:$CountryName then $$StrByCharCode:245 else $CountryName</SET>
                    </FIELD>
                    <FIELD NAME="FldMailingPincode">
                        <SET>if $$IsEmpty:$PinCode then $$StrByCharCode:245 else $PinCode</SET>
                    </FIELD>
                    <FIELD NAME="FldPAN">
                        <SET>if $$IsEmpty:$IncomeTaxNumber then $$StrByCharCode:245 else $IncomeTaxNumber</SET>
                    </FIELD>
                    <FIELD NAME="FldGSTN">
                        <SET>if $$IsEmpty:$Partygstin then $$StrByCharCode:245 else $Partygstin</SET>
                    </FIELD>
                    <FIELD NAME="FldGSTRegistrationType">
                        <SET>if $$IsEmpty:$Gstregistrationtype then $$StrByCharCode:245 else $Gstregistrationtype</SET>
                    </FIELD>
                    <FIELD NAME="FldGSTTypeOfSupply">
                        <SET>if $$IsEmpty:$Gsttypeofsupply then $$StrByCharCode:245 else $Gsttypeofsupply</SET>
                    </FIELD>
                    <FIELD NAME="FldGSTDutyHead">
                        <SET>if $$IsEmpty:$Gstdutyhead then $$StrByCharCode:245 else $Gstdutyhead</SET>
                    </FIELD>
                    <FIELD NAME="FldTaxRate">
                        <SET>if $$IsEmpty:$RateOfTaxCalculation then 0 else $RateOfTaxCalculation</SET>
                    </FIELD>
                    <FIELD NAME="FldEOL">
                        <SET>â€ </SET>
                    </FIELD>
                    <COLLECTION NAME="MyCollection">
                        <TYPE>Ledger</TYPE>
                    </COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>