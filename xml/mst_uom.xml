<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:ASCII</SVEXPORTFORMAT>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>MyPart</PARTS>
                    </FORM>
                    <PART NAME="MyPart">
                        <LINE>MyLine</LINE>
                        <REPEAT>MyLine : MyCollection</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLine">
                        <FIELDS>FldGuid,FldSymbol,FldFormalName,FldIsSimpleUnit,FldBaseUnits,FldAdditionalUnits,FldConversion,FldEOL</FIELDS>
                    </LINE>
                    <FIELD NAME="FldGuid">
                        <SET>$Guid</SET>
                    </FIELD>
                    <FIELD NAME="FldSymbol">
                        <SET>if $$IsEmpty:$Symbol then $$StrByCharCode:245 else ($$StringFindAndReplace:$Symbol:'"':'""')</SET>
                    </FIELD>
                    <FIELD NAME="FldFormalName">
                        <SET>if $$IsEmpty:$FormalName then $$StrByCharCode:245 else ($$StringFindAndReplace:$FormalName:'"':'""')</SET>
                    </FIELD>
                    <FIELD NAME="FldIsSimpleUnit">
                        <SET>if $IsSimpleUnit then 1 else 0</SET>
                    </FIELD>
                    <FIELD NAME="FldBaseUnits">
                        <SET>if $$IsEmpty:$BaseUnits then $$StrByCharCode:245 else ($$StringFindAndReplace:$BaseUnits:'"':'""')</SET>
                    </FIELD>
                    <FIELD NAME="FldAdditionalUnits">
                        <SET>if $$IsEmpty:$AdditionalUnits then $$StrByCharCode:245 else ($$StringFindAndReplace:$AdditionalUnits:'"':'""')</SET>
                    </FIELD>
                    <FIELD NAME="FldConversion">
                        <SET>if $$IsEmpty:$Conversion then 0 else $Conversion</SET>
                    </FIELD>
                    <FIELD NAME="FldEOL">
                        <SET>â€ </SET>
                    </FIELD>
                    <COLLECTION NAME="MyCollection">
                        <TYPE>Unit</TYPE>
                    </COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>